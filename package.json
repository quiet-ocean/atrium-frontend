{
  "name": "atrium",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.6.2",
    "@mui/material": "^5.6.2",
    "@reduxjs/toolkit": "^1.8.1",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.4",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.40",
    "@types/react-dom": "^17.0.13",
    "@types/react-router-dom": "^5.3.3",
    "@types/shelljs": "^0.8.11",
    "bootstrap": "^5.1.3",
    "buffer": "^6.0.3",
    "dotenv": "^16.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "immer": "^9.0.12",
    "near-api-js": "^0.44.2",
    "near-cli": "^3.2.0",
    "nodemon": "^2.0.15",
    "phaser": "^3.55.2",
    "prettier": "^2.6.0",
    "react": "^17.0.2",
    "react-bootstrap": "^2.2.1",
    "react-date-picker": "^8.4.0",
    "react-datepicker": "^4.7.0",
    "react-dom": "^17.0.2",
    "react-multi-carousel": "^2.8.0",
    "react-redux": "^8.0.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.0",
    "sass": "^1.49.9",
    "shelljs": "^0.8.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "build": "npm run build:contract && npm run build:web",
    "build:contract": "node contract/compile.ts",
    "build:contract:debug": "node contract/compile.ts --debug",
    "start:web": "react-scripts start",
    "build:web": "react-scripts build",
    "test:web": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:deploy:contract": "near dev-deploy",
    "deploy:contract": "near deploy",
    "deploy": "npm run build && npm run deploy:contract",
    "prestart": "npm run build:contract:debug && npm run dev:deploy:contract",
    "start": "echo The app is starting! It will automatically open in your browser when ready && npm run start:web",
    "dev": "nodemon --watch contract/src -e rs --exec \"npm run start\"",
    "test": "npm run build:contract:debug && cd contract && cargo test -- --nocapture && cd .. && jest test --runInBand",
    "lint": "eslint --fix --ext .ts,.tsx .",
    "init": "node contract/init.ts"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.26",
    "@types/react-datepicker": "^4.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.3.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  }
}
